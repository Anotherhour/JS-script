[Panel]
# macOS不生效
# > EDC_Surge-Panel
# 显示运行时间及MitM等功能开关状态
# 点击刷新为重载配置
EDC_Surge-Panel = script-name=EDC_Surge-Panel,update-interval=1

[Script]
# 显示运行时间及MitM等功能开关状态
EDC_Surge-Panel = type=generic,script-path= https://raw.githubusercontent.com/Anotherhour/JS-script/main/Time.js,argument=icon=crown.fill&color=#f6c970
